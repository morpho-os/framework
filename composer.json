{
    "name": "morpho-os/framework",
    "description": "Framework for building web-sites, web-applications, web-IDEs, command-line applications.",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "keywords": [
        "morpho",
        "ide",
        "framework",
        "sdk",
        "mvc",
        "rest",
        "restful",
        "core",
        "module"
    ],
    "require": {
        "php-64bit": ">=7.4",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "ext-tokenizer": "*",
        "morpho-os/zf-fork": "dev-master",
        "monolog/monolog": "dev-master",
        "nikic/fast-route": "dev-master",
        "nikic/php-parser": "dev-master",
        "psr/log": "dev-master",
        "symfony/finder": "dev-master",
        "symfony/process": "dev-master",
        "psr/simple-cache": "^1.0@dev",
        "willdurand/negotiation": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master",
        "php-webdriver/webdriver": "dev-main"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:morpho-os/zf-fork.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:morpho-os/fast-route.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Morpho\\": "lib/"
        },
        "files": [
            "lib/Base/autoload.php",
            "lib/Db/autoload.php",
            "lib/Debug/autoload.php",
            "lib/Error/autoload.php",
            "lib/App/autoload.php",
            "lib/Code/autoload.php",
            "lib/Caching/autoload.php"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "support": {
        "issues": "https://github.com/morpho-os/framework/issues"
    }
}
