{
    "name": "morpho-os/framework",
    "description": "PHP 7 framework to build PHP applications: Web-sites, Web-applications, Web-IDEs, Command-line applications.",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "keywords": [
        "morpho",
        "ide",
        "framework",
        "sdk",
        "mvc",
        "rest",
        "restful",
        "core",
        "module"
    ],
    "require": {
        "php-64bit": "~7.2",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "ext-tokenizer": "*",
        "monolog/monolog": "dev-master",
        "nikic/fast-route": "dev-master",
        "nikic/php-parser": "dev-master",
        "psr/log": "dev-master",
        "symfony/finder": "dev-master",
        "symfony/process": "dev-master",
        "zendframework/zend-http": "dev-master",
        "zendframework/zend-mail": "dev-master",
        "zendframework/zend-math": "dev-master",
        "zendframework/zend-stdlib": "dev-master",
        "psr/simple-cache": "^1.0@dev",
        "willdurand/negotiation": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master",
        "facebook/webdriver": "dev-community"
    },
    "autoload": {
        "psr-4": {
            "Morpho\\": "lib/"
        },
        "files": [
            "lib/Base/autoload.php",
            "lib/Debug/autoload.php",
            "lib/Error/autoload.php",
            "lib/App/autoload.php",
            "lib/Code/autoload.php"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "support": {
        "issues": "https://github.com/morpho-os/framework/issues"
    }
}
