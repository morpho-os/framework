{
    "name": "morpho-os/framework",
    "description": "PHP 7 framework to build PHP applications: Web-sites, Web-application, Web-IDEs, Command-line applications.",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "keywords": [
        "morpho",
        "ide",
        "framework",
        "sdk",
        "mvc",
        "rest",
        "restful",
        "core",
        "module"
    ],
    "require": {
        "php": "7.*",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-pdo_mysql": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "nikic/php-parser": "dev-master",
        "nikic/fast-route": "dev-master",
        "monolog/monolog": "dev-master",
        "zendframework/zend-http": "~2.5",
        "zendframework/zend-uri": "~2.5",
        "zendframework/zend-filter": "~2.5",
        "zendframework/zend-json": "~2.5",
        "zendframework/zend-mail": "~2.5",
        "zendframework/zend-stdlib": "~2.5",
        "zendframework/zend-validator": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Morpho\\": "lib/Morpho/"
        },
        "files": [
            "lib/Morpho/Base/autoload.php",
            "lib/Morpho/Debug/autoload.php",
            "lib/Morpho/Error/autoload.php",
            "lib/Morpho/Core/autoload.php",
            "lib/Morpho/Web/autoload.php",
            "lib/Morpho/Cli/autoload.php"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "support": {
        "issues": "https://github.com/morpho-os/framework/issues"
    }
}
