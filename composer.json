{
    "name": "morpho-os/framework",
    "description": "Framework for building web-sites, web-applications, web-IDEs, command-line applications.",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "keywords": [
        "morpho",
        "ide",
        "framework",
        "sdk",
        "mvc",
        "rest",
        "restful",
        "core",
        "module"
    ],
    "require": {
        "php-64bit": ">=8.0",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-pcntl": "*",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-reflection": "*",
        "ext-session": "*",
        "ext-spl": "*",
        "ext-tokenizer": "*",
        "ext-iconv": "*",
        "monolog/monolog": "dev-main",
        "nikic/fast-route": "dev-master",
        "nikic/php-parser": "4.10.4",
        "psr/log": "dev-master",
        "psr/simple-cache": "^1.0@dev",
        "willdurand/negotiation": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0",
        "php-webdriver/webdriver": "dev-main",
        "phpstan/phpstan": "^0.12.0@dev",
        "vimeo/psalm": "dev-master",
        "phan/phan": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Morpho\\": "lib/"
        },
        "files": [
            "lib/Base/autoload.php",
            "lib/Tech/autoload.php",
            "lib/Error/autoload.php",
            "lib/App/autoload.php",
            "lib/Caching/autoload.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Morpho\\Test\\": "test"
        }
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "support": {
        "issues": "https://github.com/morpho-os/framework/issues"
    }
}
