#!/usr/bin/env bash

set -euo pipefail

#SCRIPT_FILE_NAME="$(basename "$0")"
SCRIPT_DIR_PATH="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
#SCRIPT_FILE_PATH="$SCRIPT_DIR_PATH/$SCRIPT_FILE_NAME"

baseDirPath="$SCRIPT_DIR_PATH/.."
phpUnitBinDirPath="$baseDirPath/vendor/phpunit/phpunit"
phpUnitBinFilePath="$phpUnitBinDirPath/phpunit"
phpUnitConfigFilePath="$baseDirPath/test/phpunit.xml"

#$phpUnitBinFilePath --version

#php -f $phpUnitBinFilePath -- $([[ -n "${TRAVIS-}" ]] && echo '--debug') -c "$phpUnitConfigFilePath" "$@"


set +e
echo -----------------------------nc---------------------------------------------
nc -help
echo -----------------------------lsof-------------------------------------------
lsof --help
echo -------------------------------kill-----------------------------------------
kill --help
echo --------------------------------killall---------------------------------------
killall --help
echo ---------------------------------geckodriver---------------------------------------
geckodriver --help
echo -------------------------------------java----------------------------------
java -version
echo --------------------------------------printf----------------------------------
printf "%s" OK
echo --------------------------------------------------------------------------