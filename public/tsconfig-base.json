{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "forceConsistentCasingInFileNames": true,
        "inlineSourceMap": true,
        "jsx": "preserve",
        "lib": ["es5", "es2015", "dom"],
        "moduleResolution": "node",
        "module": "commonjs",
        "newLine": "lf",
        "noEmitHelpers": true,
        "importHelpers": true,
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noUnusedLocals": false,
        "preserveConstEnums": true,
        "removeComments": true,
        "strict": true,
        "target": "es2015",
        "incremental": true
    },
    "files": [],
    "include": []
}
